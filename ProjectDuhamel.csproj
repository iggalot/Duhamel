<Project Sdk="Godot.NET.Sdk/4.3.0">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'android' ">net7.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'ios' ">net8.0</TargetFramework>
    <EnableDynamicLoading>true</EnableDynamicLoading>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="models\items\data\" />
    <Folder Include="models\items\graphics\" />
    <Folder Include="models\monsters\data\" />
    <Folder Include="models\rooms\data\" />
    <Folder Include="models\rooms\graphics\" />
    <Folder Include="models\spells\data\" />
    <Folder Include="scenes\" />
  </ItemGroup>
</Project>